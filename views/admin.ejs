    <div class="form-row">
      <div class="form-panel">

          <div class="card bg-light mb-3" >
            <div class="card-header"><h2>Create a new post</h2></div>
            <div class="card-body">

              <form method="POST" action="/admin">

                <div class="form-body">

                  <div class="form-body-inside-left">

                    <div class="form-group row">
                      <label for="listOfCars" class="col-sm-2 col-form-label">Brand</label>
                      <div class="col-sm-7">
                        <input type="text" class="form-control" id="listOfCars" name="listOfCars" placeholder="">
                      </div>
                    </div>
                    <div class="form-group row">
                      <label for="model" class="col-sm-2 col-form-label">model</label>
                      <div class="col-sm-7">
                        <input type="text" class="form-control" id="model" name="model" placeholder="">
                      </div>
                    </div>
                    <div class="form-group row">
                      <label for="inputText3" class="col-sm-2 col-form-label">Year</label>
                      <div class="col-sm-7">
                        <input type="text" class="form-control" id="year" name="year" placeholder="">
                      </div>
                    </div>

                    <div class="form-group row">
                      <label for="inputText3" class="col-sm-2 col-form-label">Price</label>
                      <div class="col-sm-7">
                        <input type="text" class="form-control" id="price" name="price" placeholder="">
                      </div>
                    </div>
                    <div class="form-group row">
                      <label for="inputText3" class="col-sm-2 col-form-label">Color</label>
                      <div class="col-sm-7">
                        <input type="text" class="form-control" id="color" name="color" placeholder="">
                      </div>
                    </div>

                  </div>

                  <div class="form-body-inside-right">
                    <div class="form-group row">
                      <label for="inputText3" class="col-sm-3 col-form-label">Mileage</label>
                      <div class="col-sm-7">
                        <input type="text" class="form-control" id="mileage" name="mileage" placeholder="">
                      </div>
                    </div>
                    <div class="form-group row">
                      <label for="inputText3" class="col-sm-3 col-form-label">Transmission</label>
                      <div class="col-sm-7">
                        <input type="text" class="form-control" id="transmission" name="transmission" placeholder="">
                      </div>
                    </div>
                    <div class="form-group row">
                      <label for="inputText3" class="col-sm-3 col-form-label">Fuel Type</label>
                      <div class="col-sm-7">
                        <input type="text" class="form-control" id="fuelType" name="fuelType" placeholder="">
                      </div>
                    </div>

                    <div class="form-group row">
                      <label for="inputText3" class="col-sm-3 col-form-label">Image</label>
                      <div class="col-sm-7">
                        <div class="custom-file">
                          <input type="file" class="custom-file-input" id="img" name="img" aria-describedby="inputGroupFileAddon01">
                          <label class="custom-file-label" for="img">Choose file</label>
                        </div>
                      </div>
                    </div>

                    <div class="form-group row">
                      <div class="col-sm-10 " >
                        <button type="submit" class="btn btn-primary float-right">POST</button>
                      </div>
                    </div>
                  </div>
                </div>
              </form>
            </div>
          </div>

      </div>
    </div>

    <script>
      $('#img').on('change',function(e){
          $(this).next('.custom-file-label').html(e.target.files[0].name);
      })
    </script>

